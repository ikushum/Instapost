<!DOCTYPE html>

<div id="user_profile_page">

<div class="prof_header">
<%= render 'shared/profile_header.html.erb' %>
</div>
<div class="prof_post_box">

  <div class="row">

    <% @user.posts.each do |post| %>

        <div class="col-xs-6" >
          <%= image_tag post.photo, class: 'prof_show'%>
        </div>

    <% end %>

  </div>
</div>
</div>